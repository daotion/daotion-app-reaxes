{
	"compilerOptions" : {
		"allowJs" : true ,
		"allowSyntheticDefaultImports" : true ,
		"allowUnusedLabels" : true ,
		"baseUrl" : "." ,
		"declaration" : true ,
		"declarationDir" : "src/common/Global DTS" ,
		"declarationMap" : true ,
		"esModuleInterop" : true ,
		"experimentalDecorators" : true ,
		"emitDeclarationOnly" : true ,
		"jsx" : "react" ,
		"resolveJsonModule" : true ,
		"strictFunctionTypes" : false ,
		"strict" : false ,
		"strictBindCallApply" : false ,
		"strictNullChecks" : false ,
		"allowUnreachableCode" : true ,
		"lib" : [
			"ES5" ,
			"ES6" ,
			"ES2015" ,
			"ES7" ,
			"ES2016" ,
			"ES2017" ,
			"ES2018" ,
			"ESNext" ,
			"DOM" ,
			"DOM.Iterable" ,
			"WebWorker" ,
			"ScriptHost" ,
			"ES2015.Core" ,
			"ES2015.Collection" ,
			"ES2015.Generator" ,
			"ES2015.Iterable" ,
			"ES2015.Promise" ,
			"ES2015.Proxy" ,
			"ES2015.Reflect" ,
			"ES2015.Symbol" ,
			"ES2015.Symbol.WellKnown" ,
			"ES2016.Array.Include" ,
			"ES2017.object" ,
			"ES2017.Intl" ,
			"ES2017.SharedMemory" ,
			"ES2017.String" ,
			"ES2017.TypedArrays" ,
			"ES2018.Intl" ,
			"ES2018.Promise" ,
			"ES2018.RegExp" ,
			"ESNext.AsyncIterable" ,
			"ESNext.Array" ,
			"ESNext.Intl" ,
			"ESNext.Symbol" ,
			"react"
		] ,
		"module" : "CommonJS" ,
		"moduleResolution" : "node" ,
		"noEmit" : true ,
		"noImplicitAny" : false ,
		"noImplicitReturns" : false ,
		"noImplicitUseStrict" : false ,
		"noUnusedLocals" : false ,
		"noUnusedParameters" : false ,
		"paths" : {
			"*/*.ts" : [
				"*"
			] ,
			"common/*" : [
				"src/common/*"
			] ,
			"common/utils" : [
				"src/common/utils/index.ts"
			] ,
			"common/utils/*" : [
				"src/common/utils/*"
			] ,
			"common/pureComponents" : [
				"src/common/pureComponents/index.ts"
			] ,
			"common/pureComponents/*" : [
				"src/common/pureComponents/*"
			] ,
			"common/requests" : [
				"src/common/requests/index.ts"
			] ,
			"common/env" : [
				"src/common/env.ts"
			] ,
			"common/routes" : [
				"src/common/routes/index.tsx"
			] ,
			"common/Public/*" : [
				"src/common/Public/*"
			],
			"common/Auth" : [
				"src/common/Auth/Controller.tsx"
			],
			"common/mobxState" : [
				"src/common/mobxState/index"
			] 
			
		} ,
		"preserveConstEnums" : true ,
		"removeComments" : true ,
		"sourceMap" : true ,
		"target" : "ES2020" ,
		"watch" : true ,
		"allowSyntheticDefaultImports" : true
	} ,
	"include" : [
		"**/*.ts" ,
		"**/*.tsx" ,
		"src/*" ,
		"src" ,
	
	] ,
	"exclude" : [
		"node_modules" ,
		"dist" ,
		".git" ,
		".idea" ,
		".svn" ,
		"tsconfig.json"
	]
}
